cc_fuzz {
    name: "mtp_fuzzer",
    srcs: [
        "mtp_fuzzer.cpp",
        "MtpMockDatabase.cpp",
    ],
    shared_libs: [
	"libmtp",
	"libbase",
	"liblog",
	"libutils",
    ],
    cflags: [
        "-Wall",
        "-Wextra",
        "-Werror",
        "-DMTP_DEVICE",
        "-Wno-unused-parameter",
    ],
    dictionary: "mtp_fuzzer.dict",
    corpus: ["corpus/*"],

    fuzz_config: {

        cc: ["jameswei@google.com"],
        componentid: 1344,
        acknowledgement: [
            "Grant Hernandez of Google",
        ],
    },
}
